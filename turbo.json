{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                "build/**",
                ".svelte-kit/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}